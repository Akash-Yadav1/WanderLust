<!-- success flash -->
<% if (success && success.length) { %>
<div class="alert alert-success alert-dismissible fade show col6" role="alert">
  <%= success %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %>

<!-- faliure flash -->
<% if (error && error.length) { %>
<div class="alert alert-danger alert-dismissible fade show col6" role="alert">
  <%= error %>
  <button
    type="button"
    class="btn-close"
    data-bs-dismiss="alert"
    aria-label="Close"
  ></button>
</div>
<% } %>
